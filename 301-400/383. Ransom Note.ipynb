    "class Solution:\n",
    "    def canConstruct(self, ransomNote: str, magazine: str) -> bool:\n",
    "        r = Counter(ransomNote)\n",
    "        m = Counter(magazine)\n",
    "\n",
    "        for i in r.keys():\n",
    "            if i not in m.keys() or r[i] > m[i]:\n",
    "                return False\n",
    "        return True"
   ]
  }
