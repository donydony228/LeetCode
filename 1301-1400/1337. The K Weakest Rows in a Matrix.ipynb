    "class Solution:\n",
    "    def kWeakestRows(self, mat: List[List[int]], k: int) -> List[int]:\n",
    "        soldiers = [(sum(s), i) for i, s in enumerate(mat)]\n",
    "\n",
    "        soldiers.sort(reverse=False, key=lambda x: x[0])\n",
    "\n",
    "        return [soldiers[i][1] for i in range(k)]"
   ]
  }
