    "class Solution:\n",
    "    def isAnagram(self, s: str, t: str) -> bool:\n",
    "        # 長度不同直接返回False\n",
    "        if len(s) != len(t):\n",
    "            return False\n",
    "        \n",
    "        # 將字符串排序後比較\n",
    "        return sorted(s) == sorted(t)"
   ]
